{
    "max_length": 500,
    "char_limit": 10,
    "scope":"DeepPyramid",
    "num_classes":33,
    "is_train":true,

    "with_char": false,
    "char_embedding":"conv",
    "char_emb_dim": 20,
    "char_lstm_dim": 40,

    "batch_size": 32,
    "max_epochs": 1000,
    "dropout_rate": 0.5,
    "learning_rate": 0.0001,
    "optimizer": "adam",
    "lambda_l2": 0.0,
    "grad_clipper": 10.0,

    "context_layer_num": 1,
    "context_lstm_dim": 100,

    "slstm_layer_num": 5,

    "rnn":"lstm",
    "random_initialize":false,
    "step":5,
    "slstm_hidden_size":300,
    "weight_decay":1e-5,

    "highway_layer_num": 1,
    "with_highway": true,
    "with_match_highway": true,
    "with_aggregation_highway": true,

    "use_cudnn": false,
    "with_center_loss":false,

    "with_moving_average": 0.999,

    "loss":"focal_loss_multi_v1",

    "alpha":0.5,
    "gamma":2.0,
    "name":"focal_loss_multi_v1",
    "center_gamma":0.001,

    "with_word_drop":false,
    "word_drop_rate":0.7,

    "scale":30,
    "margin":0.35,

    "l2_normalization":false,

    "elmo_pca":false,

    "num_filters":128,
    "max_pool_size":5,
    "encoder_type":"multi_head_attn",
    "num_blocks":2,
    "hidden_units":600,
    "num_heads":4,

    "output_folder_name":"multi_head_attn",

    "filter_sizes":[2,3,4,5],
    "stride_length":1,
    "pooling_strategy":"hier",
    "hpcnn_filter_size":3,
    "hpcnn_number_filters":64,
    "num_repeat":4,
    "hidden_size":256
}
